{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install --network-timeout 100000",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
  }
} 